{
  "manifest_version": 2,

  "name": "Inboxnil",
  "description": "A Chrome extension to help you get to inbox zero",
  "version": "1.0",
  "permissions": [
    "tabs", "https://mail.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["inboxnil.js"]
    }
  ]
}
